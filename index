<script id="jsbin-javascript">
var d, h, m, s, color;
function tiempo() {
	d = new Date();
	h = d.getHours();
	m = d.getMinutes();
	s = d.getSeconds();
	
	//agregar 0 a las horas menos a 10
	if(h <= 9) h = '0'+h;
	if(m <= 9) m = '0'+m;
	if(s <= 9) s = '0'+s;
	
	color = "#"+h+(m+5)+s;
     
        //color al fondo
	    document.body.style.background = color;
	//repetir cada medio segundo
	setTimeout(tiempo, 1000);
}

var pagina="http://google.com.bo";
function redireccionar(){
  location.href=pagina;
}
window.onload = tiempo();
setTimeout("redireccionar()", 5000);
</script>
 
 
 
<script id="jsbin-source-javascript" type="text/javascript">var d, h, m, s, color;
function tiempo() {
	d = new Date();
	h = d.getHours();
	m = d.getMinutes();
	s = d.getSeconds();
	
	//agregar 0 a las horas menos a 10
	if(h <= 9) h = '0'+h;
	if(m <= 9) m = '0'+m;
	if(s <= 9) s = '0'+s;
	
	color = "#"+h+(m+5)+s;
     
        //color al fondo
	    document.body.style.background = color;
	//repetir cada medio segundo
	setTimeout(tiempo, 1000);
}

var pagina="http://google.com.bo";
function redireccionar(){
  location.href=pagina;
}
window.onload = tiempo();
